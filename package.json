{
  "name": "gardr-validator-web",
  "version": "0.0.18",
  "description": "The Web ui for the garðr validator",
  "main": "server.js",
  "scripts": {
    "build-browser-js": "sh ./build-browser-js.sh",
    "build": "./node_modules/.bin/grunt subgrunt && ./node_modules/.bin/bower install && npm run build-browser-js",
    "watch-js": "./node_modules/.bin/watchify    client/main.js -o client/built/bundle.js -dv",
    "start": "npm run build && node server.js",
    "test": "npm run build && ./node_modules/.bin/mocha test/**/*.test.js --reporter list"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gardr/validator-web.git"
  },
  "author": "Sveinung Røsaker <sveinung.rosaker@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gardr/validator-web/issues"
  },
  "dependencies": {
    "gardr": "0.5.17-SNAPSHOT",
    "gardr-validator": "0.0.10",
    "hapi": "1.19.4",
    "grunt-subgrunt": "~0.2.2",
    "grunt": "~0.4.1",
    "grunt-cli": "~0.1.9",
    "hoek": "~1.2.0",
    "request": "~2.27.0",
    "handlebars": "~1.1.2",
    "browserify": "~2.35.4",
    "domready": "~0.2.13",
    "dom-events": "~0.1.1",
    "concat-stream": "~1.2.0",
    "moment": "~2.4.0",
    "node-uuid": "~1.4.1",
    "async": "~0.2.9",
    "winston": "~0.7.2",
    "mkdirp": "~0.3.5",
    "bower": "~1.2.7",
    "brace": "~0.1.3",
    "clean-css": "~2.0.2",
    "htmlparser-to-html": "0.0.5",
    "htmlparser2": "~3.3.0",
    "soupselect": "~0.2.0",
    "convict": "~0.1.1",
    "css-parse": "~1.6.0",
    "uglify-js": "~2.4.3",
    "w3c-blob": "0.0.1",
    "estraverse": "~1.5.0",
    "esprima": "~1.0.4"
  },
  "devDependencies": {
    "proxyquire": "~0.5.1",
    "mocha": "~1.13.0",
    "buster-assertions": "~0.10.4",
    "watchify": "~0.3.0"
  }
}
