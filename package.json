{
  "name": "gardr-validator-web",
  "version": "0.2.4",
  "description": "The Web ui for the garðr validator",
  "main": "server.js",
  "scripts": {
    "build-browser-js": "sh ./scripts/build-browser-js.sh",
    "watch-js": "sh ./scripts/watch-browser-js.sh",
    "build": "./node_modules/.bin/bower install && npm run build-browser-js",
    "start": "npm run build && node server.js",
    "start-dev": "npm run watch-js & sleep 3 && NODE_ENV=development ./node_modules/.bin/nodemon",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha test/**/*.test.js --reporter list"
  },
  "engines": {
    "node": "0.10.x",
    "npm": "1.2.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gardr/validator-web.git"
  },
  "author": {
    "name": "Sveinung Røsaker",
    "email": "sveinung.rosaker@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gardr/validator-web/issues"
  },
  "dependencies": {
    "gardr-validator": "^0.2.1",
    "document-write-html": "0.1.3",
    "hapi": "^4.1.2",
    "hoek": "^2.1.0",
    "request": "^2.34.0",
    "handlebars": "~1.3.0",
    "browserify": "^4.1.3",
    "domready": "^1.0.4",
    "dom-events": "^0.1.1",
    "moment": "~2.5.0",
    "async": "~0.2.9",
    "mkdirp": "~0.3.5",
    "bower": "~1.2.7",
    "brace": "^0.2.1",
    "convict": "~0.4.1",
    "uglify-js": "~2.4.8",
    "lodash": "~2.4.1",
    "levelup": "~0.18.3",
    "leveldown": "~0.10.2",
    "node-uuid": "~1.4.1",
    "winston": "^0.7.2",
    "catbox": "^2.2.1",
    "catbox-memory": "^1.0.1",
    "joi": "^3.1.0",
    "decompress-zip": "0.0.6",
    "colors": "^0.6.2",
    "gardr-host": "^0.4.0",
    "gardr-ext": "^0.4.0"
  },
  "devDependencies": {
    "proxyquire": "^0.6.0",
    "watchify": "^0.6.1",
    "nodemon": "^1.0.19",
    "mock-fs": "^2.3.0",
    "mocha": "^1.18.2",
    "expect.js": "^0.3.1"
  }
}
